{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "document": true,
    "window": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "no-console": 0,
    "object-curly-spacing": 0,
    "max-len": 0,
    "require-jsdoc": 0,
    "no-invalid-this": 0,
    "prefer-rest-params": 0,
    "no-unused-vars": 0,
    "arrow-parens": 0,
    "no-trailing-spaces": 0,
    "linebreak-style": 0,
    "one-var": 0,
    "new-cap": 0,
    "no-tabs": 0,
    "no-mixed-spaces-and-tabs": 0,
    "camelcase": 0,
    "no-case-declarations": 0,
    "no-constant-condition": 0,
    "no-unreachable": 0,
    "no-empty": 0,
    "no-irregular-whitespace": 0,
  }
}
